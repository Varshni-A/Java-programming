import java.util.Scanner;
public class main{
    public static void main(String[] args){
        Scanner scanner=new Scanner(System.in);
        for(int i=1;i<=2;i++){
            System.out.println("Outer loop"+i);
            for(int j=1;j<=3;j++){
            System.out.println(i*j);
            }
        }
    }
}
